<div class="header">
    <div class="navbar navbar-default" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <a class="navbar-brand" href="/" style=
                "padding-top: 0"><img alt="Home" src=
                "http://dummyimage.com/80x50/ffffff/000&amp;text=Logo"
                style="width: 100%"></a>
            </div>
            <ul class="nav navbar-nav">
                <li><a href="/">Home</a></li>
                <li><a href="/about">About</a></li>
                <li><a href="/contact">Contact</a></li>
                <li><a href="/testimonials">Testimonials</a></li>
                <% if !user_signed_in? %>
                <li>
                <%= link_to('Login', new_user_session_path)  %>
                </li>
                <% end %>
            </ul>

              <% if user_signed_in? %>
                <ul class="nav navbar-nav navbar-right">
                  <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><%= current_user.email %><span class="caret"></span></a>
                    <ul class="dropdown-menu">
                      <li>
                        <div style="padding: 3px 20px;">
                          <%= user_role %>
                          <%= raw generate_activity_user_labels %>
                          <%# <span class="label label-default">SuperUser</span> %>
                        </div>
                      </li>
                      <% if user_role == "tenant" && !current_user.tenant.nil? %>
                      <li><a href="<%= edit_tenant_path(current_user.tenant) %>">Edit Profile</a></li>
                      <li><a href="<%= tenant_path(current_user.tenant) %>">See Tenant Information</a></li>
                      <% end %>
                      <li><a href="<%= user_dashboard_url %>">View Dashboard</a></li>
                      <li role="separator" class="divider"></li>
                      <li><a href="<%= rentals_path %>">View Rentals</a></li>
                      <li><%= link_to 'Add Testimonials', new_testimonial_path %></li>
                      <li><a href="#">File Complaint</a></li>
                      <li><a href="#">View Pending Complaints</a></li>
                      <li><%= link_to('Logout', destroy_user_session_path, :method => :delete) %></li>
                    </ul>
                  </li>
                </ul>
              <% else %>

              <% end %>
        </div>
    </div>
    <div class="container">
      <div class="row">
        <div class="alert alert-success" role="alert">
          <div>
            <p class="notice"><%= raw notice %></p>
            <p><%= alert %></p>
          </div>
        </div>
      </div>
    </div>
</div>
<div class="container">
<%= yield %>
</div>

<div class="footer">
    <div class="container">
        <div class="row">
            <div class="col-sm-4">
                <h4>About Us:</h4>
                <h4></h4>
