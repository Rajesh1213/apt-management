<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th></th>
      <th>Name</th>
      <th>Member Type</th>
      <th>Marital Status</th>
      <th>Gender</th>
      <th>Date of Birth</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% rental.tenant_family_members.each do |tenant_family_member| %>
    <tr class="info">
      <td>Member</td>
      <td><%= tenant_family_member.name %></td>
      <td><%= tenant_family_member.member_type %></td>
      <td><%= tenant_family_member.marital_status %></td>
      <td><%= tenant_family_member.gender %></td>
      <td><%= tenant_family_member.dob %></td>
      <td><%= link_to 'Show', rental_tenant_family_member_path(rental, tenant_family_member) %></td>
      <td><%= link_to 'Edit', edit_rental_tenant_family_member_path(rental, tenant_family_member) %></td>
      <td><%= link_to 'Destroy', [rental, tenant_family_member], method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
    <tr>
      <td colspan="10">
        <div class="col-md-12 well">
          <div class="text-center">
            <%= link_to "Add Family Member for Apartment  #{}#{rental.apartment.id}", new_rental_tenant_family_member_path(rental), class: "btn btn-default"%>
          </div>

        </div>

      </td>
    </tr>
  </tbody>
</table>
