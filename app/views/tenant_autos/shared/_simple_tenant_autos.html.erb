<table class="table table-striped table-hover table-condensed">
  <thead>
    <tr>
      <th></th>
      <th>AUTO ID</th>
      <th>License Number</th>
      <th>Make</th>
      <th>Year</th>
      <th>Color</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% rental.tenant_autos.each do |tenant_auto| %>
    <tr class="info">
      <td>AUTO</td>
      <td><%= tenant_auto.id %></td>
      <td><%= tenant_auto.license_number %></td>
      <td><%= tenant_auto.auto_make %></td>
      <td><%= tenant_auto.auto_model %></td>
      <td><%= tenant_auto.auto_year %></td>
      <td><%= tenant_auto.auto_color %></td>
      <td><%= link_to 'Show', rental_tenant_auto_path(rental, tenant_auto) %></td>
      <td><%= link_to 'Edit', edit_rental_tenant_auto_path(rental, tenant_auto) %></td>
      <td><%= link_to 'Destroy', [rental, tenant_auto], method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
    <tr>
      <td colspan="10">
        <div class="col-md-12 well">
          <div class="text-center">
            <%= link_to "Register Car for Apartment  #{}#{rental.apartment.id}", new_rental_tenant_auto_path(rental), class: "btn btn-default"%>
          </div>

        </div>

      </td>
    </tr>
  </tbody>
</table>
